{
  "name": "new-bee-rich",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "clean": "rimraf ./node_modules .cache ./build ./public/build && npm i",
    "build": "react-router build",
    "build:db": "npx prisma generate",
    "dev": "cross-env NODE_ENV=development node ./server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "react-router typegen && tsc",
    "format:db": "npx prisma format",
    "update:db": "npx prisma db push",
    "seed": "npx tsx prisma/seed.ts",
    "reset:db": "rimraf ./prisma/dev.db && npm run update:db && npm run seed"
  },
  "dependencies": {
    "@mjackson/form-data-parser": "^0.9.1",
    "@prisma/client": "^6.12.0",
    "@react-router/express": "^7.7.0",
    "@react-router/fs-routes": "^7.7.0",
    "@react-router/node": "^7.7.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "isbot": "^5.1.27",
    "morgan": "^1.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prisma": "^6.12.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
