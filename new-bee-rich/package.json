{
  "name": "new-bee-rich",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "clean": "rimraf ./node_modules .cache ./build ./public/build && npm i",
    "build": "run-s \"build:*\"",
    "build:remix": "remix vite:build",
    "build:db": "npx prisma generate",
    "dev": "node ./server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "format:db": "npx prisma format",
    "update:db": "npx prisma db push",
    "seed": "npx tsx prisma/seed.ts",
    "reset:db": "rimraf ./prisma/dev.db && npm run update:db && npm run seed"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@remix-run/css-bundle": "^2.16.8",
    "@remix-run/express": "^2.16.8",
    "@remix-run/node": "^2.16.8",
    "@remix-run/react": "^2.16.8",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.8",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prisma": "^6.12.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
